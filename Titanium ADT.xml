	@param {String} sql - SQL update query
	@return {resultset} returns a resultset
*/
function SQLUpdate(sql) {
	//connection string in the format: jdbc:jtds:sqlserver://servername:port/dbname
	var sqlserver = &apos;jdbc:sqlserver://&apos; + configurationMap.get(&apos;Titanium Database Server&apos;) + 
	&quot;;databaseName=&quot; + configurationMap.get(&apos;Titanium Database Name&apos;) + &quot;;ssl=require;&quot;;
	//logger.info(&quot;update&quot;);

