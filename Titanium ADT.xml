            <properties class="com.mirth.connect.model.codetemplates.BasicCodeTemplateProperties">
              <type>FUNCTION</type>
              <code>/**
	Perform an SQL query on the Titanium database

	@param {Any} sql - 
	@return {resultset} results of query
*/
function SQLSelect(sql) {
	//connection string in the format: jdbc:sqlserver://servername:port;databasename=dbname
	var sqlserver = &apos;jdbc:sqlserver://&apos; + configurationMap.get(&apos;Titanium Database Server&apos;) + 
	&quot;;databaseName=&quot; + configurationMap.get(&apos;Titanium Database Name&apos;) + &quot;;ssl=require;&quot;;

	var dbConn = DatabaseConnectionFactory.createDatabaseConnection(	
		&apos;com.microsoft.sqlserver.jdbc.SQLServerDriver&apos;,
		sqlserver,	
		configurationMap.get(&apos;Titanium Username&apos;),	
		configurationMap.get(&apos;Titanium Password&apos;)
		);

	var results = dbConn.executeCachedQuery(sql);
	dbConn.close();
	return results;
}</code>
            </properties>
